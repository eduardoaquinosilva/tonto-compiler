package Subkind_Pattern

kind ClassName
subkind SubclassNameOne specializes ClassName
subkind SubclassNameTwo specializes ClassName


disjoint complete genset Kind_Subkind_Genset_Name { 
    general ClassName
    specifics SubclassNameOne, SubclassNameTwo
}
// "complete" is optional, but "disjoint" applies to subkinds
